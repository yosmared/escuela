{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Student</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ entity.lastname }}</td>
            </tr>
            <tr>
                <th>Identification</th>
                <td>{{ entity.identification }}</td>
            </tr>
            <tr>
                <th>Citybirth</th>
                <td>{{ entity.cityBirth }}</td>
            </tr>
            <tr>
                <th>Birthdate</th>
                <td>{{ entity.birthdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{{ entity.createat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entity.telephone }}</td>
            </tr>
            <tr>
                <th>Schoolorigin</th>
                <td>{{ entity.schoolOrigin }}</td>
            </tr>
            <tr>
                <th>Viruela</th>
                <td>{{ entity.viruela }}</td>
            </tr>
            <tr>
                <th>Polio</th>
                <td>{{ entity.polio }}</td>
            </tr>
            <tr>
                <th>Tifus</th>
                <td>{{ entity.tifus }}</td>
            </tr>
            <tr>
                <th>Tetano</th>
                <td>{{ entity.tetano }}</td>
            </tr>
            <tr>
                <th>Sarampion</th>
                <td>{{ entity.sarampion }}</td>
            </tr>
            <tr>
                <th>Disease</th>
                <td>{{ entity.disease }}</td>
            </tr>
            <tr>
                <th>Explain</th>
                <td>{{ entity.explain }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('student') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('student_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
