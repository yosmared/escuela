<!DOCTYPE html>
<html lang="es">
<head>
	{% block head %} 
		{% block metainfo %}
			<meta charset="utf-8">
			<meta http-equiv="X-UA-Compatible" content="IE=edge">
			<meta name="viewport" content="width=device-width, initial-scale=1.0">
			<meta name="description" content="">
			<meta name="author" content="">
		{% endblock %}

		{% block stylesheets %}
	        <link href="{{ asset('bundles/commons/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
	        <link href="{{ asset('bundles/commons/fonts/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" />
	        <!-- <link href="{{ asset('bundles/commons/css/style.css') }}" type="text/css" rel="stylesheet" /> -->
	        <link href="{{ app.request.scheme ~'://' ~ app.request.httpHost ~ asset('bundles/commons/css/style.css') }}" type="text/css" rel="stylesheet" />
	    {% endblock %}

	    {% block javascripts %}
	        <script src="{{ asset('bundles/commons/js/jquery-1.10.2.min.js') }}" type="text/javascript"></script>
		    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
		    <!--[if lt IE 9]>
		      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
		    <![endif]-->
		    <script src="{{ asset('bundles/commons/js/bootstrap.min.js') }}" type="text/javascript"></script>	
	    {% endblock %}
    {% endblock %}
	<title>

		{% block title %}{{ "Sistema de Gesti√≥n Escolar | "~page_title|trans }}{% endblock %}
	</title>
</head>

<body>         
	{% block body %}
	    
	    {# Navbar Site header #}
	    {% block site_header %}{% endblock %}	
		
		{# Navbar header #}
		{% block navbar_header %}{% endblock %}
				
		{#  Side navbar and content #}
		<div class="container">
	   		<div class="row">
			{% block side_navbar %}{% endblock %}
			{% block content %}
				
  			{% endblock %}
			</div>
		</div>
		
		{#  Footer #}	    		
		{% block footer %} {% endblock %}		
		
	{% endblock %}

	{# Script section at the bottom of the document #}
	{% block bottomscript %}{% endblock %}
</body>
</html>